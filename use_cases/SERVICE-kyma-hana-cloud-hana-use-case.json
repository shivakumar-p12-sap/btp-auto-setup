{
    "$schema": "https://raw.githubusercontent.com/SAP-samples/btp-setup-automator/main/libs/btpsa-usecase.json",
    "services": [
      {
        "name": "hana-cloud",
        "plan": "hana",
        "category": "SERVICE",
        "targetenvironment": "kymaruntime",
        "parameters": {
          "data": {
              "edition": "cloud",
              "memory": 30,
              "serviceStopped": false,
              "storage": 120,
              "systempassword": "mySAP123",
              "vcpu": 2,
              "versionIndicator": "",
              "whitelistIPs": [
                  "0.0.0.0/0"
              ]
          }
        },
        "createServiceKeys": [
          "hana-cloud-hana"
        ],
        "api_resource_uri": {
          "APITest": "no",
          "authMethod": "oauth",
          "serviceID": "SERVICE-443"
        }
      }
    ],
    "enableAPITest": true
  }
  